{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "standard-with-typescript",
				"prettier"
    ],
		"settings": {
			"react": {
				"version": "detect"
			}
		},
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
				"project": ["./tsconfig.json"]
    },
    "plugins": [
        "react",
				"prettier"
    ],
    "rules": {
			"prettier/prettier": "error",
			"react/react-in-jsx-scope": "off"
    }
}
